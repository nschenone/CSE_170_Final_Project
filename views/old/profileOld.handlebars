<html>

<head>
	<title>My Profile</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<!-- Bootstrap -->
	<link href="../css/bootstrap.css" rel="stylesheet">
	<link href="../css/bootstrap-theme.css" rel="stylesheet">
	<link href="../css/style.css" rel="stylesheet">
</head>

<body>
	<div class="bg">
		<div class="header">
			<div class="header-text">My Profile</div>
		</div>

		<div class="profile-container">
			<img src="../images/profile_pic_placeholder.png" class="profile-image"> <br><br>

			<label for="fname">Name:</label>
			Jimmy <br>

			<label for="lname">Email:</label>
			jimmy@ucsd.edu <br>

			<label for="lname">Contact Info:</label>
			(123) 456-7890 <br>

			<a href="#">
				<button type="submit" name="class-button" class="profile-button">
					<div class="form-text">
						Update Password
					</div>
				</button>
			</a>

			<a href="/">
				<button type="submit" name="class-button" class="profile-button">
					<div class="form-text">
						Logout
					</div>
				</button>
			</a>
		</div>


	</div>


	<div class="footer">
		<div class="footer-buttons">
			<a href="../search/">
				<button type="submit" name="search" class="footer-image-search">
					<img src="../images/search_classes_resize.png">
				</button>
			</a>

			<a href="/myClasses">
				<button type="submit" name="classes" class="footer-image-classes">
					<img src="../images/my_classes_resize.png">
				</button>
			</a>

			<a href="#">
				<button type="submit" name="profile" class="footer-image-profile-active">
					<img src="../images/my_profile_resize.png">
				</button>
			</a>
		</div>
	</div>
	</div>

	<script src="../js/jquery-1.11.0.js"></script>
	<script src="../js/bootstrap.js"></script>
	<script src="../js/home.js"></script>
</body>

</html>